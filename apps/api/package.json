{
  "name": "@degen/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--dns-result-order=ipv4first' tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist node_modules",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx src/scripts/seed.ts",
    "init-protocol": "tsx src/scripts/init-protocol.ts",
    "init-markets": "tsx src/scripts/init-markets.ts",
    "setup-fee-recipient": "tsx src/scripts/setup-fee-recipient.ts"
  },
  "dependencies": {
    "@solana/web3.js": "^1.91.0",
    "@solana/spl-token": "^0.4.0",
    "@coral-xyz/anchor": "^0.29.0",
    "fastify": "^4.26.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "drizzle-orm": "^0.30.0",
    "tweetnacl": "^1.0.3",
    "bs58": "^5.0.0",
    "zod": "^3.22.0",
    "pino": "^8.19.0",
    "dotenv": "^16.4.0",
    "@degen/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.10",
    "drizzle-kit": "^0.20.0",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  }
}


